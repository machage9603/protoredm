{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "jsx": "react", // or "react-jsx" if using React 17+
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "allowSyntheticDefaultImports": true,
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ]
        }
    }
}